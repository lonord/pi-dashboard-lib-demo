{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "semicolon":[
            true,
            "never"
        ],
        "quotemark": [
            true,
            "single",
            "jsx-double"
        ],
        "indent": [
            true,
            "tabs"
        ],
        "no-console": false
    },
    "rulesDirectory": []
}